
            
#Perimeter_2D
<PerimeterView>:
    name:"peri_2d"
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        FirstBoxLayout:
            size_hint_y: None
            height: self.minimum_height
        #square
            TitleLabel:
                text:'Square'
                size_hint_y: None
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        radius:[58,58,0,0]
                        pos: self.pos
                        size: self.size 
            BoxLayout:
                size_hint_y: None
                Label1:
                    text:'Length of a side'
                    size_hint_y: None
                MyTextInput:
                    id:p_sqr_s
                    size_hint_y: None
                    hint_text:"Side"
            BoxLayout:
                size_hint_y: None
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        radius:[0,0,58,58]
                        pos: self.pos
                        size: self.size
                Label1:
                    text:'Perimeter'
                    size_hint_y: None
                    color:.5,.5,.5,1
                Label:
                    text:''
                    id:p_res_sqr
                    color:[1,0,0,1]
                    size_hint_y: None
            MyButton:
                text:'[b]Calculate Perimeter of Square[/b]'
                size_hint_y: None
                on_release:
                    root.p_sqr()
            Label:
                canvas.before:
                    Color:
                        rgba:  1, 1, 1, 1
                    Rectangle:
                        pos: self.pos
                        size: self.size[0], 1
        #rectangle
            TitleLabel:
                text:'Rectangle'
