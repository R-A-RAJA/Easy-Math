
#Surface area_3D
<SurfaceAreaView>:
    name:"area_3d"
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        FirstBoxLayout:
            size_hint_y: None
            height: self.minimum_height
        #Cube
            TitleLabel:
                text:'Cube'
                size_hint_y: None
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        radius:[58,58,0,0]
                        pos: self.pos
                        size: self.size
            BoxLayout:
                size_hint_y: None
                Label1:
                    text:'Length of a side'
                    size_hint_y: None
                MyTextInput:
                    id:a_cub
                    size_hint_y: None
                    hint_text:"Side"
            BoxLayout:
                orientation:'horizontal'
                size_hint_y: None
                spacing:20
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        radius:[0,0,58,58]
                        pos: self.pos
                        size: self.size
                Label1:
                    id:l_a_res_cub
                    text:'Result'
                    size_hint_y: None
                    color:.5,.5,.5,1
                Label:
                    text:''
                    id:a_res_cub
                    color:[1,0,0,1]
                    size_hint_y: None
            BoxLayout:
                spacing:20
                size_hint_y: None
                FloatLayout:
                    MyButton:
                        text:'[b]CSA of Cube[/b]'
                        size_hint_y: None
                        on_release:
                            root.csa_cub()
                FloatLayout:
                    MyButton:
                        text:'[b]TSA of Cube[/b]'
                        size_hint_y: None
                        on_release:
                            root.tsa_cub()
            Label:
                canvas.before:
                    Color:
                        rgba:  1, 1, 1, 1
                    Rectangle:
                        pos: self.pos
                        size: self.size[0], 1
