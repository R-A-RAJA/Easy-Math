
#volume_3D
<VolumeView>:
    name:"vol_3d"
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        FirstBoxLayout:
            size_hint_y: None
            height: self.minimum_height
        #Cube
            TitleLabel:
                text:'Cube'
                size_hint_y: None
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        radius:[58,58,0,0]
                        pos: self.pos
                        size: self.size
            BoxLayout:
                size_hint_y: None
                Label1:
                    text:'Length of a side'
                    size_hint_y: None
                MyTextInput:
                    id:v_cub
                    size_hint_y: None
                    hint_text:"Side"
            BoxLayout:
                orientation:'horizontal'
                size_hint_y: None
                canvas.before:
                    Color:
                        rgba: 1,1,1,1
                    RoundedRectangle:
                        radius:[0,0,58,58]
                        pos: self.pos
                        size: self.size
                spacing:20
                Label1:
                    text:'Volume'
                    size_hint_y: None
                    color:.5,.5,.5,1
                Label:
                    text:''
                    id:v_res_cub
                    color:[1,0,0,1]
                    size_hint_y: None
            MyButton:
                text:'[b]Volume of Cube[/b]'
                size_hint_y: None
                on_release:
                    root.v_cub()
            Label:
                canvas.before:
                    Color:
                        rgba:  1, 1, 1, 1
                    Rectangle:
                        pos: self.pos
                        size: self.size[0], 1
